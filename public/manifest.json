{
  "name": "My-Extension",
  "version": "1.0",
  "manifest_version": 2,
  "browser_action": {},
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["inject.js"]
    }
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "web_accessible_resources": ["index.html"],
  "permissions": ["activeTab"]
}
